package  com.scripts.attractor{		import flash.display.Sprite;	import flash.events.*;	import com.scripts.sound.*;		import GroggerTiltle;	import StartGame;	import highScore;	import directions;		public class attractorStarter extends Sprite	{			private var myStartSound:soundStarter;				public var mystartGame:StartGame = new StartGame;		public var myHighScore:highScore = new highScore;		public var mydirections:directions = new directions;				public function attractorStarter():void		{					var myGroggerTiltle:GroggerTiltle = new GroggerTiltle;			addChild(myGroggerTiltle);			myGroggerTiltle.y =280; 			myGroggerTiltle.x = 155; 									addChild(mystartGame);			mystartGame.y = 350; 			mystartGame.x = 200;			mystartGame.addEventListener(MouseEvent.CLICK,StartGamecomplete);									addChild(myHighScore);			myHighScore.y = 420; 			myHighScore.x = 200;			myHighScore.addEventListener(MouseEvent.CLICK,highScorecomplete);									addChild(mydirections);			mydirections.y = 480; 			mydirections.x = 200;			mydirections.addEventListener(MouseEvent.CLICK,directionscomplete);					}				private function StartGamecomplete(e:Event):void {			dispatchEvent(new Event("gameCreateCaller"));			myStartSound = new soundStarter("dp_frogger_coin.",false);        }				private function highScorecomplete(e:Event):void {			dispatchEvent(new Event("getHighScoreCaller"));			myStartSound = new soundStarter("dp_frogger_coin.",false);        }		private function directionscomplete(e:Event):void {			dispatchEvent(new Event("directionsCaller"));			myStartSound = new soundStarter("dp_frogger_coin.",false);        }	}}