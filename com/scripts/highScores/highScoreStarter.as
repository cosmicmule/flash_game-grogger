package com.scripts.highScores{	import flash.display.Sprite;	import flash.text.*;	import com.scripts.xmlLoader.xmlStarter;	import flash.events.*;	import scoreBoardMc;	import com.scripts.scoreBoard.namelist		public class highScoreStarter extends Sprite	{		public var myxmlObj:xmlStarter 		private var XMlArray:Array = new Array ();		private var objArray:Array = new Array();		public var myscoreBoardMc:scoreBoardMc; 		private const localPadding = 120;						public function highScoreStarter(level:Sprite,setter:String):void		{			myxmlObj = new xmlStarter("http://flash.mannydasilva.com/grogger/main.php?action=",setter,level);			myxmlObj.addEventListener("XML_is_Complete", XMlComplete);						myscoreBoardMc = new scoreBoardMc;			addChild(myscoreBoardMc);			myscoreBoardMc.highScoreClose.addEventListener(MouseEvent.CLICK,clickDispatch)		}		public function clickDispatch(e:Event):void		{			dispatchEvent(new Event("moveScore"));		}		public function XMlComplete(event:Event):void		{			var holder = myxmlObj.XMLReturn();			trace(holder[0].user[3].name)						for(var C:Number = 0; C < holder.user.length(); C++)			{			XMlArray[C] = new Array (holder[0].user[C].name,holder[0].user[C].points);			}				trace(XMlArray)			for(var B:Number = 0; B < XMlArray.length; B++)			{								var mynamelist:namelist = new namelist;				mynamelist.genericTextField.textImport.autoSize = TextFieldAutoSize.LEFT;								var localString:String = XMlArray[B][0];				mynamelist.genericTextField.textImport.text = localString.substring(0, 12)				addChild(mynamelist);							objArray.push(mynamelist);				mynamelist.x = 30;				mynamelist.y =localPadding + B*25;								mynamelist = new namelist;				mynamelist.genericTextField.textImport.autoSize = TextFieldAutoSize.LEFT;				mynamelist.genericTextField.textImport.text = XMlArray[B][1];				mynamelist.y = localPadding + B*25;				mynamelist.x =  300;				addChild(mynamelist);								objArray = [];							}							}	}}