package com.scripts.xmlLoader{	import flash.display.MovieClip;	import com.scripts.xmlLoader.xmlLoad;	import flash.display.*;	import flash.events.*;	import flash.net.*;		public class xmlStarter extends MovieClip	{		public var myXml:xmlLoad;		public var myNewArray:XMLList = new XMLList ();		public function xmlStarter(NewUrl:String,pointer:String,level)		{						myXml = new xmlLoad(NewUrl+pointer);			level.addChild(myXml);			myXml.addEventListener (Event.COMPLETE,xmlComplete);		}				private function xmlComplete (event:Event):void		{			myXml.removeEventListener (Event.COMPLETE,xmlComplete);			myNewArray = myXml.getXML();			dispatchEvent(new Event("XML_is_Complete"))		}				public function XMLReturn ():XMLList		{			return myNewArray;		}	}	}