package com.scripts.waterVehicle{	import flash.display.Sprite;	import watervehicle;	import flash.events.Event;		public class waterVehicleStarter extends Sprite	{		private var vehicleChooose:Number;		public var vehicleOj:Object = new Object;		private var loopLocal:Number = 410;		private var loopCount:Number = 1;		private var mySwitch:Boolean= true;		private var gameSpeed = 10;				public function waterVehicleStarter ():void		{			vehicleType();			addEventListener(Event.ENTER_FRAME,vehicleFrame);		}				private function vehicleType ():void		{			for(var Z:int = 0; Z <= 4; Z++)			{								loopLocal -= 50;												for(var C:int = 1; C <= 2; C++){										vehicleChooose = Randomvehicle();										if(vehicleChooose == 1){						var mywatervehicle:watervehicle = new watervehicle;						addChild(mywatervehicle);						vehicleOj[loopCount] = mywatervehicle 					}						vehicleOj[loopCount].y = loopLocal;										if(C == 1){						vehicleOj[loopCount].x = Math.round(Math.random() * 900);					}else{						vehicleOj[loopCount].x = (vehicleOj[loopCount-1].x - vehicleOj[loopCount-1].width) - Math.random() * 900;					}					loopCount++;				}											}		}				private function vehicleFrame (e:Event):void		{			for (var s:String in vehicleOj)			{				if(s=="1" || s=="2" || s=="5" || s=="6" || s=="9" || s=="10"){					mySwitch = true}else{mySwitch = false}								if(mySwitch == true){					if(vehicleOj[s].x < 0 - vehicleOj[s].width){vehicleOj[s].x = stage.stageWidth }else{vehicleOj[s].x -= gameSpeed }				}								if(mySwitch != true){					if(vehicleOj[s].x > stage.stageWidth){vehicleOj[s].x = 0 - vehicleOj[s].width}else{vehicleOj[s].x += gameSpeed }				}			}		}				private function Randomvehicle ():Number		{			var vehiclerandom:Number;			//vehiclerandom = Math.round(Math.random() * 4);			vehiclerandom = 1;			return vehiclerandom;		}		public function changeSpeed ()		{			gameSpeed+=1;		}		public function remove():void		{			removeEventListener(Event.ENTER_FRAME,vehicleFrame);		}	}}